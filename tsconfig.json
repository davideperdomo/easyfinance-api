{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/src",
    "rootDir": "./src"
  },
  "include": ["src/**/*", "api/**/*", ".env", "fb-sa.json"],
  "exclude": ["node_modules", "**/*.spec.ts", "**/*.test.ts"]
}